<!--If user login success, alerts success message-->
{{#if success}}
<p class="alert alert-success">{{success}}</p>
{{/if}}

{{#if links}}
<ul>
  {{#each links }}
  <li>
    <div class="row">
        <div class="col s12 m12">
          <div class="card blue lighten-5">
            <div class="card-content white-black center">
              <span class="card-title">
                <a class="waves-effect waves-light btn-large amber darken-1" href="/link/{{id}}">{{title}}
              </a> - <a href={{link}}>{{link}}</a></span>
            </div>
          </div>
        </div>
      </div>
  </li>
  {{/each}}
</ul>
{{/if}}
</div>
